    Layout = "~/Areas/Admin/Views/Shared/AdminLayout.cshtml";
}

<head>
    <style>
        img{
            height: 250px;
            max-width: 250px;
        }
    </style>
</head>

<h2 class="bg-success">Thông tin chi tiết</h2>

@Html.Partial("PVDelDetProduct", Model)
<div class="form-group">
    <div class="col-md-offset-3 col-md-9" >
        @Html.ActionLink("Cập nhật", "Edit", new { id = Model.ProductID }, new { @class = "btn btn-primary" })
        @Html.ActionLink("Quay lại danh sách", "Index", null, new { @class = "btn btn-primary" })
    </div>
</div>
